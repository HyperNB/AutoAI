{
  "manifest_version": 3,
  "name": "AutoAI beta - Auto suggestion for chatgpt",
  "version": "0.2",
  "description": "AutoAI beta - Auto Suggestion for chatgpt and gemini",
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
      "service_worker": "worker.js",
      "type": "module"
   },
  "icons": {
  },
  "content_scripts": [
    {
      "matches": ["*://chat.openai.com/"],
      "js": ["background.js"]
    }
  ]
}
