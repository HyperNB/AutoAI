{
  "manifest_version": 2,
  "name": "AutoAI beta - Auto suggestion for chatgpt",
  "version": "0.1",
  "description": "AutoAI beta - Auto Suggestion for chatgpt and gemini",
  "permissions": [
    "https://search.brave.com/api/suggest*"
  ],
  "icons": {
    "512": "icons/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["*://chat.openai.com/"],
      "js": ["background.js"]
    }
  ]
}
